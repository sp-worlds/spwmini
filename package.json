{
  "name": "spwmini",
  "version": "1.2.0",
  "description": "SPWorlds Miniapp creation toolkit",
  "types": "dist/index.d.ts",
  "exports": {
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./middleware": {
      "import": "./dist/middleware.mjs",
      "require": "./dist/middleware.js"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "scripts": {
    "build": "tsup src/client.ts src/middleware.ts src/types.ts --tsconfig tsconfig.build.json --format cjs,esm --dts",
    "format": "prettier -w ."
  },
  "files": [
    "/dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sp-worlds/spwmini.git"
  },
  "keywords": [
    "SPWorlds",
    "SPWorlds",
    "API",
    "spworlds",
    "spw-mini",
    "spwmini",
    "SPWMini",
    "SPWMiniapp",
    "SP"
  ],
  "author": {
    "name": "SuhEugene",
    "url": "https://suheugene.ru"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sp-worlds/spwmini/issues"
  },
  "homepage": "https://spworlds.ru",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4"
  }
}
